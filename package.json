{
  "name": "meu-projeto",
  "version": "1.0.0",
  "scripts": {
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "npm --prefix front_end start",
    "start:backend": "npm --prefix back_end start",
    "build": "concurrently \"npm run build:frontend\" \"npm run build:backend\"",
    "build:frontend": "npm --prefix front_end run build",
    "build:backend": "npm --prefix back_end run build",
    "install": "npm install --legacy-peer-deps"
  },
  "devDependencies": {
    "concurrently": "^7.0.0"
  }
}